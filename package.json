{
  "name": "my-movies-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone SE'",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "bundle-ios": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle --assets-dest ios/MyMoviesApp",
    "bundle-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn run lint",
      "pre-commit": "yarn run lint"
    }
  },
  "dependencies": {
    "@apollo/client": "^3.3.15",
    "@gympass/yoga": "^7.21.2",
    "@react-native-async-storage/async-storage": "^1.13.2",
    "@react-native-community/masked-view": "^0.1.7",
    "@react-native-firebase/analytics": "^12.2.0",
    "@react-native-firebase/app": "^12.1.0",
    "@react-native-firebase/crashlytics": "^12.6.0",
    "@react-native-picker/picker": "^2.2.0",
    "@react-navigation/bottom-tabs": "^6.x",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "@react-navigation/stack": "^6.0.11",
    "fbjs": "^3.0.0",
    "graphql": "^15.5.0",
    "lottie-react-native": "^4.0.2",
    "react": "^17.0.1",
    "react-native": "^0.64.2",
    "react-native-config": "^1.3.3",
    "react-native-device-info": "^8.1.7",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.9.0",
    "react-native-svg": "^12.1.0",
    "reactotron-react-native": "^4.0.3",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/react-native": "^7.0.2",
    "@types/jest": "^24.0.24",
    "@types/react-native": "^0.60.25",
    "@types/react-test-renderer": "16.9.1",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.5",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.2",
    "metro-react-native-babel-preset": "^0.64.0",
    "prettier": "^1.19.1",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "^17.0.1",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.3"
  }
}
